{
    "type": "library",
    "name": "volmarg/personal-management-system-io-bridge",
    "description": "Bridge between PMS-IO and PMS",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "guzzlehttp/guzzle": "^7.2",
        "lukasoppermann/http-status": "^3.1",
        "monolog/monolog": "^2.2",
        "symfony/cache": "v5.4.0",
        "symfony/config": "v5.4.0",
        "symfony/console": "5.*",
        "symfony/dependency-injection": "v5.4.0",
        "symfony/dotenv": "v5.4.0",
        "symfony/error-handler": "v5.4.0",
        "symfony/event-dispatcher": "v5.4.0",
        "symfony/filesystem": "v5.4.0",
        "symfony/finder": "v5.4.0",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "v5.4.0",
        "symfony/http-foundation": "v5.4.0",
        "symfony/http-kernel": "v5.4.0",
        "symfony/routing": "v5.4.0",
        "symfony/string": "v5.4.0",
        "symfony/var-dumper": "v5.4.0",
        "symfony/var-exporter": "v5.4.0",
        "symfony/yaml": "v5.4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\PmsIo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
